# Scrapybara API Key
SCRAPYBARA_API_KEY=

# Mock Service Configuration
# Set USE_MOCK=true to enable fake/mock responses for debugging
NEXT_PUBLIC_USE_MOCK=false


# Delay in milliseconds between mock responses (simulates network delay)
# Default: 500ms
MOCK_DELAY_MS=500

# Fake instance ID to return when starting an instance
# Default: mock-instance-12345
NEXT_PUBLIC_MOCK_INSTANCE_ID=mock-instance-12345


